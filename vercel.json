{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/:path*"
      }
    ],
    "redirects": [],
    "headers": [],
    "functions": {
      "api/*": {
        "runtime": "nodejs18.x"
      }
    }
  }
  